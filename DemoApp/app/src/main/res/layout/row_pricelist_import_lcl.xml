                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/col_of"
                android:textColor="@color/black"
                android:textSize="17sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/guideline2"
                app:layout_constraintTop_toBottomOf="@id/tv_row_price_import_lcl_cargo" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/local_pol"
                android:textSize="17sp"
                android:textColor="@color/black"
                app:layout_constraintEnd_toEndOf="@id/guideline2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_row_price_import_lcl_of" />

            <TextView
                android:id="@+id/tv_row_price_import_lcl_local_pol"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/local_pol"
                android:textColor="@color/black"
                android:textSize="17sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/guideline2"
                app:layout_constraintTop_toBottomOf="@id/tv_row_price_import_lcl_of" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
