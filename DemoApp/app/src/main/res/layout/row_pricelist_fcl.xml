                app:layout_constraintTop_toBottomOf="@id/tv_row_price_asia_line" />

            <TextView
                android:id="@+id/tv_row_price_asia_notes1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/col_su4"
                android:textColor="@color/black"
                android:textSize="17sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/guideline2"
                app:layout_constraintTop_toBottomOf="@id/tv_row_price_asia_line" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/col10"
                android:textSize="17sp"
                android:textColor="@color/black"
                app:layout_constraintEnd_toEndOf="@id/guideline2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_row_price_asia_notes1" />

            <TextView
